{
  "name": "aplikasirapor",
  "version": "1.0.0",
  "description": "Aplikasi Rapor Kesetaraan",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": "Muhammad Khoiruddin, S.Kom",
  "license": "ISC",
  "devDependencies": {
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4"
  },
  "build": {
    "appId": "com.mkhoiruddin.aplikasirapor",
    "productName": "Aplikasi Rapor Kesetaraan",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "installerIcon": "assets/icon.ico"
    }
  }
}